{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/grabber/(.*)",
      "destination": "https://video-grabber.vercel.app/$1"
    },
    {
      "source": "/grabber",
      "destination": "https://video-grabber.vercel.app/"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/favicon.ico", "dest": "/icons/favicon.ico" },
    { "src": "/index.html", "headers": { "Cache-Control": "no-cache" } },
    { "src": "/flutter_service_worker.js", "headers": { "Cache-Control": "no-cache" } },
    { "src": "/assets/(.*)", "headers": { "Cache-Control": "public, max-age=31536000, immutable" } },
    { "src": "/icons/(.*)", "headers": { "Cache-Control": "public, max-age=31536000, immutable" } },
    { "src": "/splash/(.*)", "headers": { "Cache-Control": "public, max-age=31536000, immutable" } },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}


